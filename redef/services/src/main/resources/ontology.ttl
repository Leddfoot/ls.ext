@prefix : <__BASE_URI_ONTOLOGY__> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix lvont: <http://lexvo.org/ontology#> .
@prefix ui: <http://data.deichman.no/ui#> .
@prefix values: <__BASE_URI_VALUES__> .
@prefix duo: <http://data.deichman.no/utility#> .

:Work a rdfs:Class ;
  rdfs:label "Work"@en ;
  rdfs:label "Verk"@no .

:Publication a rdfs:Class ;
  rdfs:label "Publication"@en ;
  rdfs:label "Utgivelse"@no .

:Person a rdfs:Class ;
  rdfs:label "Person"@en ;
  rdfs:label "Person"@no .

:name a rdfs:Property ;
  rdfs:label "Name"@en ;
  rdfs:label "Navn"@no ;
  rdfs:domain rdfs:Class ;
  rdfs:range xsd:string .

:creator a rdfs:Property ;
  rdfs:label "Creator"@en ;
  rdfs:label "Skaper"@no ;
  rdfs:domain :Work ;
  rdfs:range :Person .

:year a rdfs:Property ;
  rdfs:label "Year"@en ;
  rdfs:label "Årstall"@no ;
  rdfs:domain :Work ;
  rdfs:range xsd:gYear .

:recordID a rdfs:Property ;
  ui:editable false ;
  rdfs:label "Record ID"@en ;
  rdfs:label "Post ID"@no ;
  rdfs:domain :Publication ;
  rdfs:range xsd:nonNegativeInteger .

:language a rdfs:Property ;
  rdfs:label "Language"@en ;
  rdfs:label "Språk"@no ;
  rdfs:domain :Publication ;
  rdfs:range lvont:Language ;
  duo:valuesFrom values:language .

:format a rdfs:Property ;
  rdfs:label "Format"@en ;
  rdfs:label "Format"@no ;
  rdfs:domain :Publication ;
  rdfs:range duo:Format ;
  duo:valuesFrom values:format .

:publicationOf a rdfs:Property ;
  rdfs:label "Publication of"@en ;
  rdfs:label "Utgivelse av"@no ;
  rdfs:domain :Publication ;
  rdfs:range :Work .

:birth a rdfs:Property ;
  rdfs:label "Birth year"@en ;
  rdfs:label "Fødselsår"@no ;
  rdfs:domain :Person ;
  rdfs:range xsd:gYear.

:death a rdfs:Property ;
  rdfs:label "Death year"@en ;
  rdfs:label "Dødsår"@no ;
  rdfs:domain :Person ;
  rdfs:range xsd:gYear.

:cataloguing_name a rdfs:Property ;
  rdfs:label "Cataloguing name"@en ;
  rdfs:label "Katalogiseringsnavn"@no ;
  rdfs:domain :Person ;
  rdfs:range xsd:string .

:alternative_name a rdfs:Property ;
  rdfs:label "Alternative name"@en ;
  rdfs:label "Alternativt navn"@no ;
  rdfs:domain rdfs:Class ;
  rdfs:range xsd:string .

:sorting_name a rdfs:Property ;
  rdfs:label "Sorting name"@en ;
  rdfs:label "Sorteringsnavn"@no ;
  rdfs:domain :Person ;
  rdfs:range xsd:string .
  
:numeration a rdfs:Property ;
  rdfs:label "Numeration"@en ;
  rdfs:label "Nummerering"@no ;
  rdfs:domain :Person ;
  rdfs:range xsd:string .

:description a rdfs:Property ;
  rdfs:label "Description"@en ;
  rdfs:label "Beskrivelse"@no ;
  rdfs:domain rdfs:Class ;
  rdfs:range xsd:string .

:nationality a rdfs:Property ;
  rdfs:label "Nationality"@en ;
  rdfs:label "Nasjonalitet"@no ;
  rdfs:domain :Person ;
  rdfs:range :Nationality .

:gender a rdfs:Property ;
  rdfs:label "Gender"@en ;
  rdfs:label "Kjønn"@no ;
  rdfs:domain :Person ;
  rdfs:range :Gender .

:biography a rdfs:Property ;
  rdfs:label "Biography"@en ;
  rdfs:label "Biografi"@no ;
  rdfs:domain :Person ;
  rdfs:range xsd:string .

:depiction a rdfs:Property ;
  rdfs:label "Depiction"@en ;
  rdfs:label "Bilde"@no ;
  rdfs:domain rdfs:Class .
