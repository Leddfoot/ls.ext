@prefix : <http://data.deichman.no/lsext-model#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

:Work a rdfs:Class ;
  rdfs:label "Work"@en ;
  rdfs:label "Verk"@no .

:Publication a rdfs:Class ;
  rdfs:label "Publication"@en ;
  rdfs:label "Utgivelse"@no .

:Book a rdfs:Class ;
  rdfs:label "Book"@en ;
  rdfs:label "Bok"@no .

:name a rdfs:Property ;
  rdfs:label "Name"@en ;
  rdfs:label "Navn"@no ;
  rdfs:domain :Work, :Publication ;
  rdfs:range rdf:langString .

:creator a rdfs:Property ;
  rdfs:label "Creator"@en ;
  rdfs:label "Skaper"@no ;
  rdfs:domain :Work ;
  rdfs:range xsd:string .

:year a rdfs:Property ;
  rdfs:label "Year"@en ;
  rdfs:label "Årstall"@no ;
  rdfs:domain :Work ;
  rdfs:range xsd:gYear .

:biblio a rdfs:Property ;
  rdfs:label "Biblio"@en ;
  rdfs:label "Biblio"@no ;
  rdfs:domain :Work ;
  rdfs:range xsd:nonNegativeInteger .

:language a rdfs:Property ;
  rdfs:label "Language"@en ;
  rdfs:label "Språk"@no ;
  rdfs:domain :Publication ;
  rdfs:range xsd:string .

:format a rdfs:Property ;
  rdfs:label "Format"@en ;
  rdfs:label "Format"@no ;
  rdfs:domain :Publication ;
  rdfs:range xsd:string .

:publicationOf a rdfs:Property ;
  rdfs:label "Publication of"@en ;
  rdfs:label "Utgivelse av"@no ;
  rdfs:domain :Publication ;
  rdfs:range :Work .

:belongsTo a rdfs:Property ;
  rdfs:label "Belongs to"@en ;
  rdfs:label "Hører til"@no ;
  rdfs:domain :Book ;
  rdfs:range xsd:string .

:barcode a rdfs:Property ;
  rdfs:label "Barcode"@en ;
  rdfs:label "Strekkode"@no ;
  rdfs:domain :Book ;
  rdfs:range xsd:string .